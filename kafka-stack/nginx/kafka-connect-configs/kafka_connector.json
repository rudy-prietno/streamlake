{
  "name": "companyname-dbname-prod-tablename-cdc-formatdata",
  "config": {
    "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
    "plugin.name": "pgoutput",
    "database.hostname": "${secrets:secret_manager_id:hostname}",
    "database.port": "${secrets:secret_manager_id:port}",
    "database.user": "${secrets:secret_manager_id:username}",
    "database.password": "${secrets:secret_manager_id:password}",
    "database.dbname": "${secrets:secret_manager_id:database}",
    "database.server.name": "${secrets:secret_manager_id:database}",
    "slot.name": "${secrets:secret_manager_id:slotname}",
    "publication.name": "${secrets:secret_manager_id:publicationname}",
    "table.include.list": "public.tablename",
    "topic.prefix": "topic_prefix_name",
    "snapshot.mode": "never",
    "include.schema.changes": "false",
    "tasks.max": "1",
    "key.converter": "io.confluent.connect.avro.AvroConverter",
    "key.converter.schema.registry.url": "${secrets:secret_manager_id:schemaregistryurl}",
    "key.converter.schemas.enable": "true",
    "value.converter": "io.confluent.connect.avro.AvroConverter",
    "value.converter.schema.registry.url": "${secrets:secret_manager_id:schemaregistryurl}",
    "value.converter.schemas.enable": "true",
    "decimal.handling.mode": "string",
    "time.precision.mode": "adaptive_time_microseconds",
    "transforms": "unwrap",
    "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
    "transforms.unwrap.drop.tombstones": "false",
    "transforms.unwrap.add.fields": "op,ts_ms",
    "producer.max.request.size": "70485760",
    "producer.buffer.memory": "73554432",
    "producer.compression.type": "lz4",
    "retries": "5",
    "retry.backoff.ms": "3000",
    "max.batch.size": "2048",
    "max.queue.size": "8192",
    "poll.interval.ms": "3000",
    "producer.retries": "5",
    "producer.reconnect.backoff.ms": "1000",
    "producer.reconnect.backoff.max.ms": "10000",
    "producer.delivery.timeout.ms": "120000",
    "topic.creation.default.replication.factor": "3",
    "topic.creation.default.partitions": "6",
    "topic.creation.default.cleanup.policy": "delete",
    "topic.creation.default.compression.type": "lz4",
    "topic.creation.default.max.message.bytes": "70485760",
    "topic.creation.default.retention.ms": "74200000",
    "heartbeat.interval.ms": "3600000",
    "heartbeat.action.query": "INSERT INTO public.kafkaconnect_heartbeat (connector_name, last_heartbeat) VALUES ('prod-databasename-tablename-cdc-formatdata', NOW()) ON CONFLICT (connector_name) DO UPDATE SET last_heartbeat = NOW()",
    "tombstones.on.delete": "true"
  }
}
